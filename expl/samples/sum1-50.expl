int main()
{
decl
	int temp, num, counter, sum;
enddecl
begin
	temp = exposcall("Read", -1, num);
	while (num <= 100) do
		sum = 0;
		counter = 1;	
		while (counter <= num) do
			sum = sum + counter;
			counter = counter + 1;
		endwhile;	
		temp = exposcall("Write", -2, sum);
		num = num + 1;
	endwhile;
	return 0;
end
}
